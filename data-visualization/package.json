{
  "name": "sp-frontpanel-line-chart",
  "version": "0.0.1",
  "description": "GrapesJS Plugin for Dashboard",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "clean": "rimraf dist",
    "pack": "webpack --env.production",
    "build": "npm run clean && npm run pack",
    "start": "webpack-dev-server --open --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artf/grapesjs-plugin-boilerplate.git"
  },
  "keywords": [
    "plugin",
    "boilerplate",
    "wysiwyg"
  ],
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^3.2.0",
    "echarts": "^4.8.0",
    "eslint": "^5.13.0",
    "file-loader": "^6.0.0",
    "grapesjs": "^0.16.18",
    "grapesjs-blocks-basic": "^0.1.8",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.5.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^3.1.0",
    "rimraf": "^3.0.2",
    "suanpan-sdk": "^0.9.263-beta.10",
    "terser-webpack-plugin": "^3.0.8",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "url-loader": "^4.1.0"
  }
}
